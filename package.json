{
  "name": "devkeeper-typedoc",
  "description": "TypeDoc commands and API for devkeeper.",
  "version": "1.0.1",
  "author": "Özüm Eldoğan @ozum",
  "bin": {
    "devkeeper-typedoc": "./bin/run"
  },
  "bugs": "https://github.com/ozum/devkeeper-typedoc/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "concat-md": "^0.3.5",
    "execa": "^4",
    "fs-extra": "^9",
    "tmp-promise": "^3.0.2",
    "tslib": "^1",
    "typedoc": "^0.17.6",
    "typedoc-neo-theme": "^1.0.8",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-markdown": "^2.2.17"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^10",
    "@types/tmp": "^0.2.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3"
  },
  "peerDependencies": {
    "typescript": "^3.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ozum/devkeeper-typedoc",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "devkeeper-typedoc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ozum/devkeeper-typedoc",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "release": "git push --follow-tags origin master && npm publish"
  },
  "types": "lib/index.d.ts"
}
